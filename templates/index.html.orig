      0% {
        transform: translateX(7px);

      }

      50% {
        transform: translateX(15px);
      }

      100% {
        transform: translateX(7px);
      }

    }

    .opt-icon {
      animation: 2s vibrate infinite;
    }

    .select2 .select2-container .select2-container--default .select2-container--below{
      width: 100% !important;
    }
  </style>
